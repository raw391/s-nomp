    "homepage": "https://github.com/s-nomp/s-nomp",
    "bugs": {
        "url": "https://github.com/s-nomp/s-nomp/issues"
    },
    "license": "MIT",
    "author": "s-nomp dev team",
    "contributors": [
        "aayanl",
        "egyptianbman",
