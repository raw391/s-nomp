    "repository": {
        "type": "git",
        "url": "https://github.com/s-nomp/s-nomp.git"
    },
    "dependencies": {
        "async": "^2.6.4",
        "bignum": "^0.13.0",
        "body-parser": "^1.20.1",
        "colors": "^1.3.1",
