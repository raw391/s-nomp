    },
    "overrides": {
        "blake2b-wasm": "git+https://github.com/mafintosh/blake2b-wasm#7d8efa4f71d71983bc87983b25c6c9513fb9d1a0",
        "stratum-pool": {
            "equihashverify": "git+https://github.com/marcofortina/equihashverify.git"
